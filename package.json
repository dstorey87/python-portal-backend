{
  "name": "@python-portal/backend",
  "version": "1.0.0",
  "description": "Python Portal Backend API - Express.js microservice with OpenAPI 3.0 specification",
  "type": "module",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/startup.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist coverage .nyc_output",
    "docs": "swagger-jsdoc -d swagger.config.js src/routes/*.ts -o docs/api-spec.yaml",
    "docs:serve": "swagger-ui-serve docs/api-spec.yaml"
  },
  "dependencies": {
    "@python-portal/types": "^1.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "tsc-alias": "^1.8.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "jest-html-reporters": "^3.1.5",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "@python-portal/types": "^1.0.0"
  },
  "keywords": [
    "python",
    "learning",
    "education",
    "api",
    "express",
    "typescript",
    "microservice",
    "openapi",
    "swagger",
    "rest"
  ],
  "author": "Python Portal Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dstorey87/python-portal-backend.git"
  },
  "bugs": {
    "url": "https://github.com/dstorey87/python-portal-backend/issues"
  },
  "homepage": "https://github.com/dstorey87/python-portal-backend#readme",
  "files": [
    "dist",
    "docs",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}